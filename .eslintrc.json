{
  "env": {
    "browser": true
  },
  "globals": {},
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "eqeqeq": 2,
    "comma-style": [
      2,
      "first"
    ],
    "curly": 2,
    "linebreak-style": [
      2,
      "unix"
    ],
    "quotes": [
      2,
      "single"
    ],
    "array-bracket-spacing": [
      2,
      "never",
      {
        "singleValue": true,
        "objectsInArrays": true
      }
    ],
    "consistent-this": [
      2,
      "that"
    ],
    "padded-blocks": [
      2,
      "never"
    ]
  }
}
